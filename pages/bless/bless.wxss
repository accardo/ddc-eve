.selected_icon{
  width: 56rpx;
  height: 56rpx;
  position: absolute;
  top: 26rpx;
  right: 20rpx;
  background: url('data:image/png;base64,') no-repeat;
  background-size: 100%;
  display: none;
}
.bless_list.active .selected_icon{
  display: block;
}
.nextStep{
  margin-top: 0rpx;
}