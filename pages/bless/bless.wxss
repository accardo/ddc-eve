/* pages/bless/bless.wxss */
@import "../index.wxss";
.bless_title{
  font-size:36rpx;
  color: #FF697E;
  text-align: center;
  margin: 14rpx auto;
}
.bless_list{
  width: 660rpx;
  height: 108rpx;
  padding: 0 13rpx;
  line-height: 108rpx;
  border-radius:32px;
  font-size: 42rpx;
  color: #ffffff;
  text-align: center;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
  margin: 0 auto 14rpx;
  position: relative;
}
.bless_list0{
  background: #ff773b;
}
.bless_list1{
  background: #FF9A58;
}
.bless_list2{
  background: #FEB375;
}
.bless_list3{
  background: #FDC165;
}
.bless_list4{
  background: #CBCB8E;
}
.bless_list5{
  background: #A9D2A9;
}
.bless_list6{
  background: #88DAC9;
}
.bless_bottom{
  background: #ffffff;
  box-shadow:0px -20px 16px 0px rgba(146,146,146,0.2);
  padding: 32rpx 0 26rpx;
  margin-top: 20rpx;
}
.bless_input{
  width: 628rpx;
  height: 108rpx;
  line-height: 108rpx;
  padding: 0 26rpx 0 32rpx;
  margin: 0 auto 32rpx;
  border-radius:32px;
  font-size: 36rpx;
  color: #000000;
  background-color: #f5f5f5;
  position: relative;
}
.placeholder_input{
  color: #A4A7B1;
}
.placeholder_input::before{
  content: '';
  width: 36rpx;
  height: 36rpx;
  display: inline-block;
  vertical-align: middle;
  margin-right: 14rpx;
  background: url('data:image/png;base64,') no-repeat;
  background-size: 100%;
}
.placeholder_active{
  display: none;
}
